{
  "name": "breakpad-server",
  "version": "0.0.1",
  "description": "A simple breakpad server built with hapijs server and postgresql",
  "main": "index.js",
  "scripts": {
    "coveralls": "node_modules/.bin/lab -m 10000 -r lcov | ./node_modules/.bin/coveralls",
    "test": "./node_modules/.bin/lab -m 10000 -l test",
    "test-server": "./node_modules/.bin/lab -m 10000 -l test/server.js",
    "test-routes": "./node_modules/.bin/lab -m 10000 -l routes/test test/server.js",
    "test-crash-dump": "./node_modules/.bin/lab -m 10000 -l routes/test/crash_dumps.js",
    "test-stack-walk": "./node_modules/.bin/lab -m 10000 -l routes/test/stack_walk.js",
    "test-symbols": "./node_modules/.bin/lab -m 10000 -l routes/test/stack_walk.js",
    "test-login": "./node_modules/.bin/lab -m 10000 -l routes/test/login.js",
    "test-assets": "./node_modules/.bin/lab -m 10000 -l routes/test/assets.js",
    "test-cov-html": "./node_modules/.bin/lab -l -r html -o coverage.html routes/test test/server.js",
    "start": "node --harmony index.js",
    "test-jenkins": "node_modules/.bin/lab -l -r tap -o test.tap -r clover -o clover.xml test"
  },
  "engines": {
    "node": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mickelindahl/hapi_breakpad_server.git"
  },
  "keywords": [
    "hapi",
    "hapijs",
    "breakpad",
    "google",
    "crash",
    "reporter",
    "symbols",
    "c++",
    "cpp"
  ],
  "author": "Mikael Lindahl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mickelindahl/hapi_breakpad_server/issues"
  },
  "homepage": "https://github.com/mickelindahl/hapi_breakpad_server#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "boom": "^4.2.0",
    "bootstrap": "^4.0.0-alpha.5",
    "browserify": "^13.3.0",
    "bunyan": "^1.8.5",
    "bunyan-prettystream": "^0.1.3",
    "chosen-npm": "^1.4.2",
    "datatables.net": "^2.1.1",
    "datatables.net-bs4": "file:./plugins/datatables.net-bs4",
    "debug": "^2.6.0",
    "dotenv": "^4.0.0",
    "file-exists": "^2.0.0",
    "handlebars": "^4.0.6",
    "hapi": "^16.1.0",
    "hapi-auth-jwt": "^4.0.0",
    "hapi-redirect": "^1.0.3",
    "hapi-swagger": "^7.6.0",
    "hapi-waterline": "^0.1.2",
    "inert": "^4.1.0",
    "joi": "^10.2.0",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^7.2.1",
    "load-css": "^0.1.0",
    "minidump": "^0.11.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.3.0",
    "path-exists": "^3.0.0",
    "rmdir": "^1.2.0",
    "sails-memory": "^0.10.7",
    "sails-postgresql": "^0.11.4",
    "therealyou": "^1.1.0",
    "uuid": "^3.0.1",
    "vision": "^4.1.1"
  },
  "devDependencies": {
    "code": "^4.0.0",
    "coveralls": "^2.11.15",
    "lab": "^12.0.0",
    "mock-require": "^2.0.1"
  }
}
